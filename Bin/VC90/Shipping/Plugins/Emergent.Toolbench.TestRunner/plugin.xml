<?xml version="1.0" encoding="utf-8" ?>
<plugin
  xmlns="http://www.emergent.net/schemas/plugin-base-schema.xsd"  
  name="Test Runner" 
  id="Emergent.Toolbench.TestRunner"
  version="1.1.0.0"
  provider="Emergent Game Technologies"
  class="Emergent.Toolbench.TestRunner.Plugin">

  <!-- Plug-ins that wish to be testable by the test runner service should 
       define an extension for the following extension point. -->
  <extension-point id="Emergent.Toolbench.TestRunner.Testable"
                   schema="Schemas\ExtPoint-Testable.xsd"/> 
  
  <runtime>Emergent.Toolbench.TestRunner.dll</runtime>

</plugin>