<?xml version="1.0" encoding="utf-8" ?>
<plugin
  xmlns="http://www.emergent.net/schemas/plugin-base-schema.xsd"
  name="PropertyType"
  id="Emergent.Toolbench.PropertyType"
  version="1.1.0.0"
  provider="Emergent Game Technologies"
  class="Emergent.Toolbench.PropertyType.Plugin">

  <extension-point id="Emergent.Toolbench.PropertyType.PropertyType"
                   schema="Schemas\ExtPoint-PropertyType.xsd"/>

  <extension point="Emergent.Toolbench.PropertyType.PropertyType">
    <type name="Float" managed-type="System.Single" />
    <type name="Double" managed-type="System.Double" />
    <type name="Boolean" managed-type="System.Boolean" />
    <type name="Integer" managed-type="System.Int32" />
    <type name="Unsigned Integer" managed-type="System.UInt32" />
    <type name="Short" managed-type="System.Int16" />
    <type name="Unsigned Short" managed-type="System.UInt16" />
    <type name="String" managed-type="System.String" />
    <type name="Color (RGB)" managed-type="System.Drawing.Color" />
    <type name="Guid" managed-type="System.Guid" />
  </extension>

  <runtime>Emergent.Toolbench.PropertyType.dll</runtime>

</plugin>